<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MineCustomTabText" parent="TextAppearance.Design.Tab">
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
        <item name="textAllCaps">false</item>
    </style>

    <style name="errorToast">
        <item name="stColorBackground">#FFFFFF</item>
        <item name="stTextColor">@color/black</item>
        <item name="stIconStart">@drawable/warningred</item>
        <item name="stLength">LONG</item>
        <item name="stRadius">10dp</item>

    </style>
    <style name="warningToast">
        <item name="stColorBackground">#FFFFFF</item>
        <item name="stTextColor">@color/black</item>
        <item name="stIconStart">@drawable/warningg</item>
        <item name="stLength">LONG</item>
        <item name="stRadius">10dp</item>
    </style>
    <style name="successToast">
        <item name="stColorBackground">#FFFFFF</item>
        <item name="stTextColor">@color/black</item>
        <item name="stIconStart">@drawable/success</item>
        <item name="stLength">LONG</item>
        <item name="stRadius">10dp</item>
    </style>
    <style name="animationDialog">
        <item name="android:windowEnterAnimation">@anim/top_animation</item>
        <item name="android:windowExitAnimation">@anim/top_bot_animation</item>
    </style>
    <style name="animationDialog2">
        <item name="android:windowEnterAnimation">@anim/bottom_animation</item>
        <item name="android:windowExitAnimation">@anim/bottom_top_animation</item>
    </style>
    <style name="openSanFont">
        <item name="fontFamily">@font/open_sans_bold</item>
    </style>

</resources>